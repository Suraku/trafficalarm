<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="false"
        android:drawable="@drawable/state_buttonprimary_enabled"
        android:background="@drawable/ic_buttonstripe"/>
    <item
        android:state_pressed="true"
        android:state_enabled="true"
        android:drawable="@drawable/state_buttonprimary_pressed" />
